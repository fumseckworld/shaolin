{
    "name": "imperium/imperium",
    "type": "library",
    "description": "A library to manage database more simply",
    "license": "GPL-3.0-or-later",
    "support": {
       "source": "https://git.fumseck.eu/cgit/imperium/"
    },
    "authors": [
        {
            "name": "Willy Micieli",
            "email": "will.micieli@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "pragmarx/google2fa": "^2.0",
        "czproject/git-php": "^3.10",
        "sinergi/browser-detector": "^6.1",
        "intervention/image": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "^1.22",
        "robmorgan/phinx": "^0.9.0",
        "ralouphie/mimey": "^1.0",
        "spatie/db-dumper": "^2.11",
        "ext-PDO": "^7.2",
        "ext-json": "^1.6"
    },
    "autoload-dev":{
        "psr-4":{
            "Testing\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Imperium\\": "imperium/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.7",
        "phpro/grumphp": "^0.14.0",
        "codedungeon/phpunit-result-printer": "^0.22.0",
        "filp/whoops": "^2.2",
        "symfony/var-dumper": "^4.1"
    }
}
