  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.4.2",
    "ext-openssl": "*",
    "ext-gettext": "*",
    "ext-redis": "*",
    "ext-apcu": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-hash": "*",
    "ext-mbstring": "*",
    "ext-session": "*",
    "symfony/console": "^5.1",
    "symfony/process": "^5.1",
    "php-di/php-di": "^6.2",
    "fzaninotto/faker": "^1.9",
    "sinergi/browser-detector": "^6.1",
    "mobiledetect/mobiledetectlib": "^2.8",
    "vlucas/phpdotenv": "^5.0",
    "filp/whoops": "^2.7",
    "predis/predis": "^1.1",
    "scrivo/highlight.php": "v9.18.1.1",
    "erusev/parsedown": "^1.7",
    "swiftmailer/swiftmailer": "^6.2",
    "opis/closure": "^3.5",
    "ext-zend-opcache": "*"
  },
  "authors": [
    {
      "name": "Willy Micieli",
      "email": "fumseck@fumseck.org"
