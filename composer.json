{
  "name": "eywa/eywa",
  "description": "A library to make mvc websites more simply",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.4",
    "predis/predis": "^1.1",
    "symfony/console": "^5.0",
    "symfony/yaml": "^4.0",
    "swiftmailer/swiftmailer": "^6.2",
    "symfony/var-dumper": "^5.0",
    "filp/whoops": "^2.5",
    "doctrine/annotations": "^1.8",
    "ocramius/proxy-manager": "^2.2",
    "php-di/php-di": "^6.0",
    "erusev/parsedown": "^1.7",
    "scrivo/highlight.php": "^9.17",
    "fzaninotto/faker": "^1.9",
    "vlucas/phpdotenv": "^4.1",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "nesbot/carbon": "^2.27",
    "codedungeon/phpunit-result-printer": "^0.26.2",
    "gettext/gettext": "^5.2",
    "ext-gettext": "*",
    "ext-apcu": "*",
    "symfony/process": "^5.0",
    "phpunit/phpunit": "^8.5"
  },
  "authors": [
    {
      "name": "Willy Micieli",
      "email": "micieli@laposte.net"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Testing\\": "tests/",
      "App\\": "app/"
    }
  },
  "autoload": {
    "psr-4": {
      "Eywa\\": "eywa/"
    },
    "files": [
      "helpers/web.php",
      "helpers/const.php",
      "helpers/admin.php"
    ]
  },
  "scripts": {
    "test": "phpunit  --coverage-html coverage",
    "coverage" : "php -S localhost:8000 -t coverage",
    "serve" : "php -S localhost:3000 -t web",
    "send" : "git push origin --all && git push origin --tags"
  }
}
