{
  "name": "imperium/imperium",
  "type": "library",
  "description": "A library to build a modern and personalisable mvc framework",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Willy Micieli",
      "email": "micieli@laposte.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "pragmarx/google2fa": "^2.0",
    "sinergi/browser-detector": "^6.1",
    "intervention/image": "^2.4",
    "guzzlehttp/guzzle": "^6.3",
    "nesbot/carbon": "^2.0.0",
    "robmorgan/phinx": "^0.9.0",
    "ralouphie/mimey": "^1.0",
    "maximebf/debugbar": "^1.15",
    "predis/predis": "^1.1",
    "twig/twig": "^2.6",
    "vlucas/phpdotenv": "^3.1",
    "twig/extensions": "^1.5",
    "umpirsky/twig-gettext-extractor": "^1.3",
    "guzzlehttp/psr7": "^1.5",
    "symfony/http-foundation": "^4.2",
    "symfony/yaml": "^4.2",
    "swiftmailer/swiftmailer": "^6.1",
    "psr/http-server-middleware": "^1.0",
    "symfony/console": "^4.2",
    "ext-gettext": "*",
    "ext-PDO": "*",
    "ext-intl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-apcu": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "erusev/parsedown": "^1.7",
    "php-di/php-di": "^6.0",
    "doctrine/annotations": "^1.6",
    "danielstjules/stringy": "^3.1",
    "fzaninotto/faker": "^1.7",
    "ext-redis": "*",
    "scrivo/highlight.php": "^9.15"
  },
  "autoload-dev": {
    "psr-4": {
      "Testing\\": "tests/",
      "App\\": "app/"
    }
  },
  "autoload": {
    "psr-4": {
      "Imperium\\": "imperium/"
    },
    "files": [
      "helpers/web.php",
      "helpers/const.php",
      "helpers/admin.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpro/grumphp": "^0.14.3",
    "codedungeon/phpunit-result-printer": "^0.26.0",
    "filp/whoops": "^2.2",
    "symfony/var-dumper": "^4.1",
    "symfony/profiler-pack": "^1.0",
    "symfony/debug-pack": "^1.0",
    "symfony/debug": "^4.3"
  }
}
